{
  "name": "myat-kyaw-thu/laravel-api-visibility",
  "description": "Automatic API route documentation and live previewing for Laravel applications",
  "type": "library",
  "license": "MIT",
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "autoload": {
    "psr-4": {
      "myatKyawThu\\LaravelApiVisibility\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "myatKyawThu\\LaravelApiVisibility\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Andrew",
      "email": "myatkyawthu4002@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/routing": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/view": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0",
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "myatKyawThu\\LaravelApiVisibility\\ApiVisibilityServiceProvider"
      ],
      "aliases": {
        "ApiVisibility": "myatKyawThu\\LaravelApiVisibility\\Facades\\ApiVisibility"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}