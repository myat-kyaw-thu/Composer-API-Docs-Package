{
    "name": "primebeyonder/laravel-api-visibility",
    "description": "Automatic API route documentation and live previewing for Laravel applications",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Primebeyonder\\LaravelApiVisibility\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Primebeyonder\\LaravelApiVisibility\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Andrew",
            "email": "myatkyawthu4002@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/http": "^8.0|^9.0|^10.0",
        "illuminate/routing": "^8.0|^9.0|^10.0",
        "illuminate/view": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Primebeyonder\\LaravelApiVisibility\\ApiVisibilityServiceProvider"
            ],
            "aliases": {
                "ApiVisibility": "Primebeyonder\\LaravelApiVisibility\\Facades\\ApiVisibility"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
